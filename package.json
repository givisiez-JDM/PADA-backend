      "crypto": "^1.0.1",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "express-async-errors": "^3.1.1",
      "http-status-codes": "^2.2.0",
      "joi": "^17.9.1",
      "joi-password-complexity": "^5.1.0",
      "jsonwebtoken": "^9.0.0",
      "md5": "^2.3.0",
      "multer": "^1.4.5-lts.1",
      "nodemailer": "^6.9.3",
      "pg": "^8.10.0",
      "reflect-metadata": "^0.1.13",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^4.6.3",
      "typeorm": "0.3.14"
   },
   "devDependencies": {
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
