      "@types/multer": "^1.4.7",
      "@types/node": "^16.11.10",
      "@types/nodemailer": "^6.4.8",
      "@types/swagger-jsdoc": "^6.0.1",
      "@types/swagger-ui-express": "^4.1.3",
      "nodemon": "^2.0.22",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
