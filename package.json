{
   "name": "app-vacina",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "scripts": {
      "prestart": "npm run typeorm migration:run -- -d src/database/data-source.ts",
      "start": "ts-node src/index.ts",
      "dev": "nodemon src/index.ts",
